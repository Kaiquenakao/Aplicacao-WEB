<%
	String nome = request.getParameter("cxnome");
	int classe = Integer.valueOf(request.getParameter("cxclasse"));
	Float preco = Float.valueOf(request.getParameter("cxpreco"));
	
	if(classe==null && preco == null){
		out.print("valeu");
	}
	out.print("nome: " + nome + "classe: " + classe + "preco: " + preco);
%>